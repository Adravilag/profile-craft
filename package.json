{
  "name": "profilecraft",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "homepage": "https://adravilag.github.io/ProfileCraft",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "env:local": "powershell -ExecutionPolicy Bypass -File ./switch-env.ps1 local",
    "env:production": "powershell -ExecutionPolicy Bypass -File ./switch-env.ps1 production",
    "env:status": "echo \"Current Environment:\" && findstr VITE_ .env 2>nul || findstr NODE_ENV .env 2>nul || echo \"No .env file found\""
  },
  "dependencies": {
    "@babel/runtime": "^7.27.6",
    "@emailjs/browser": "^4.4.1",
    "@lexical/code": "^0.31.2",
    "@lexical/html": "^0.31.2",
    "@lexical/link": "^0.31.2",
    "@lexical/list": "^0.31.2",
    "@lexical/react": "^0.31.2",
    "@lexical/rich-text": "^0.31.2",
    "@lexical/table": "^0.31.2",
    "@tinymce/tinymce-react": "^6.1.0",
    "@types/blueimp-md5": "^2.18.2",
    "acorn": "^8.14.1",
    "axios": "^1.9.0",
    "blueimp-md5": "^2.19.0",
    "cloudinary": "^2.6.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "framer-motion": "^12.16.0",
    "highlight.js": "^11.11.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "lexical": "^0.31.2",
    "marked": "^15.0.12",
    "node-fetch": "^2.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.5.2",
    "react-markdown": "^10.1.0",
    "react-quill": "^2.0.0",
    "react-router-dom": "^7.6.1",
    "slate": "^0.114.0",
    "slate-react": "^0.114.2"
  },
  "devDependencies": {
    "@babel/parser": "^7.27.5",
    "@eslint/js": "^9.25.0",
    "@types/html2canvas": "^0.5.35",
    "@types/node": "^22.15.21",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/react-i18next": "^8.1.0",
    "@vitejs/plugin-react": "^4.4.1",
    "depcheck": "^1.4.7",
